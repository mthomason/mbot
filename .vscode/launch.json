{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [{
			"name": "mbotserv",
			"type": "python",
			"request": "launch",
			"module": "uvicorn",
			"args": [
				"main:app",
				"--reload",
				"-Xfrozen_modules=off"
			],
			"jinja": true,
			"justMyCode": true,
			"cwd": "${workspaceFolder}/backend",
			"envFile": "${workspaceFolder}/.env"
		}, {
			"name": "mbotclient-vite",
			"type": "node",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run",
				"dev"
			],
			"cwd": "${workspaceFolder}/vite-app",
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		}, {
			"name": "mbotclient-chrome",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:5173",
			"webRoot": "${workspaceFolder}/vite-app/src"
		}],
	"compounds": [{
			"name": "Launch FastAPI and Vite",
			"configurations": ["mbotserv", "mbotclient-vite", "mbotclient-chrome"]
		}]
}